			<value>Windows</value>
			<value>Linux</value>
			<value>Unix</value>
		</PlatformType>
		<Browsers name="envBrowsers">
			<value>Google Chrome</value>
			<value>Mozilla Firefox</value>
		</Browsers>
	</System>
	<Suite>
		<Package name="envPackage">
			<value>com.ch.aut.desktop</value>
